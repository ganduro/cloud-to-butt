{
	"manifest_version": 2,
	"name": "Jordan to Jorp",
	"version": "1.0",
	"description": "Makes the internet a little more bearable.",
	"icons": { "16": "images/jorp16.png",
           "48": "images/jorp64.png",
          "128": "images/jorp128.png" },
	"content_scripts":
	[
		{
			"matches": ["*://*/*"],
			"js": ["jorp.js"],
			"run_at": "document_end"
		}
	]
}
